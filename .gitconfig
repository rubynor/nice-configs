[color]
  diff = auto
  status = auto
  branch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
  name = INSERT YOUR NAME HERE
  email = INSERT YOUR GIT EMAIL HERE
[alias]
  s=status
  c=commit
  d=diff
  b=branch
  m=merge
  ch=checkout
  p=pull
[core]
  excludesfile = ~/.gitignore_global
[push]
  default = simple
[pull]
  rebase = false
